    "require": {
        "php": ">=8.1",
        "drupal/core": "^9.5 || ^10"
    },
    "require-dev": {
        "drush/drush": "^11 || ^12",
        "drupal/devel": "^5.1"
    },
    "license": "GNUv3",
    "autoload": {
