{
  "name": "strakez/drupal-entity-decorator",
  "description": "Implements a decorator pattern to access and customize typed access to an entity's properties and methods",
  "type": "drupal-module",
  "require": {
    "php": ">=8.1",
    "ramsey/collection": "dev-main",
    "loophp/collection": "dev-master"
  },
  "require-dev": {
    "drupal/core": "^9.5 || ^10",
    "drush/drush": "^11 || ^12",
    "drupal/devel": "^5.1",
    "squizlabs/php_codesniffer": "4.0.x-dev",
    "psr/log": "3.x-dev"
  },
  "license": "GPL-2.0-or-later",
  "authors": [
    {
      "name": "strakers",
      "email": "s.straker@utoronto.ca"
    }
  ],
  "minimum-stability": "dev",
  "config": {
    "preferred-install": "dist",
    "sort-packages": false,
    "platform": {
      "php": "8.1.0"
    },
    "allow-plugins": {
      "composer/installers": true,
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  },
  "autoload": {
    "files": [
      "src/Support/Utility/functions.php"
    ]
  }
}
